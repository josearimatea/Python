import pandas as pd

dados = [['Jose', 29, 'Manaus', 'M'], ['Natalia', 27, 'Passos', 'F'], ['Zeze', 40, 'Sao Paulo', 'M']]
colunas = ['Nome', 'Idade', 'Cidade', 'Sexo']
indice = range(0,3)

lista_de_dados = pd.DataFrame(data = dados, columns = colunas, index = indice)

print(lista_de_dados)

lista_de_dados.to_csv('lista_de_dados.csv')